{
  "vcpkg-configuration": {
    "overlay-ports": [
      "./extension-ci-tools/vcpkg_ports"
    ]
  },
  "dependencies": [
    {
      "name": "msgpack"
    },
    {
      "name": "curl",
      "features": [
        "http2",
        "openssl"
      ]
    },
    {
      "name": "arrow",
      "default-features": false,
      "features": [
        "filesystem",
        "flight"
      ]
    }
  ]
}